{
  "name": "@aw-web-design/components",
  "version": "0.0.3",
  "description": "A component library",
  "scripts": {
    "build:tokens": "node ./node_modules/@aw-web-design/theme/generate",
    "lint": "yarn lint:js && yarn lint:styled",
    "lint:js": "eslint -c .eslintrc packages --ext .ts,.tsx --quiet",
    "lint:styled": "stylelint \"packages/**/styled.ts\"",
    "test": "jest --silent -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AW-Web-Design/Components.git"
  },
  "author": "Andy Wilson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AW-Web-Design/Components/issues"
  },
  "homepage": "https://github.com/AW-Web-Design/Components#readme",
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "copy-webpack-plugin": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "typescript": "^3.9.6",
    "enhanced-resolve": "5.0.0-beta.7",
    "styled-components": "5.1.1",
    "styled-system": "5.1.5",
    "csstype": "2.6.11",
    "@aw-web-design/theme": "0.0.14"
  },
  "devDependencies": {
    "@types/webpack": "^4.41.19",
    "@types/webpack-env": "^1.14.0",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "copy-webpack-plugin": "^6.0.3",
    "fork-ts-checker-webpack-plugin": "^5.0.6",
    "html-webpack-plugin": "^4.2.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.4",
    "babel-eslint": "^10.0.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-standard": "^4.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "prettier": "2.0.5",
    "circular-dependency-plugin": "5.2.0",
    "@storybook/addon-docs": "5.3.19",
    "css-loader": "3.6.0",
    "mini-css-extract-plugin": "0.9.0",
    "ts-loader": "7.0.5",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.5.0",
    "jest": "26.1.0",
    "jest-cli": "26.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/**/*.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/^\/node_modules\/@orchard\/((theme)|(theme-provider))$/"
    ],
    "resolver": "./jestAliasResolver"
  }
}
