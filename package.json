{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "yarn build:tokens && tsdx build",
    "build:tokens": "node ./node_modules/@aw-web-design/theme/dist/cli.js generate",
    "test": "yarn build:tokens && tsdx test",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "yarn build:tokens && start-storybook -p 6006",
    "build-storybook": "build-storybook --docs"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "name": "@aw-web-design/components",
  "author": "The-Code-Monkey",
  "module": "dist/components.esm.js",
  "size-limit": [
    {
      "path": "dist/components.cjs.production.min.js",
      "limit": "10 KB",
      "webpack": false
    },
    {
      "path": "dist/components.esm.js",
      "limit": "10 KB",
      "webpack": false
    }
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/packages/$1"
    }
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/plugin-syntax-jsx": "7.14.5",
    "@size-limit/preset-small-lib": "6.0.3",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.3.12",
    "@storybook/addons": "6.3.12",
    "@storybook/react": "6.3.12",
    "@types/enzyme": "3.10.10",
    "@types/react": "17.0.31",
    "@types/react-dom": "17.0.10",
    "@types/styled-components": "5.1.15",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.5",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "7.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "size-limit": "6.0.4",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@aw-web-design/styled-system": "^0.1.3",
    "@aw-web-design/theme": "^0.1.0-alpha-5",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "csstype": "^3.0.9",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "memoize-one": "^6.0.0",
    "prettier": "^2.4.1",
    "react-feather": "^2.0.9",
    "react-lazy-named": "^1.2.2",
    "styled-components": "^5.3.0"
  }
}
